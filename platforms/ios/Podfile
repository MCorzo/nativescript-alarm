
platform :ios, '10.0'

//pod 'NativeScriptAlarm', :path => '/Users/ricardo/sources/repositories/nativescript-modules/ios-libs/nativescript-alarm-ios-sources'
pod 'NativeScriptAlarm', :git => 'git@github.com:mobilemindtec/nativescript-alarm-ios-sources.git'

post_install do |installer|
  installer.pods_project.targets.each do |target|
    target.build_configurations.each do |config|
      config.build_settings['SWIFT_VERSION'] = '4.1'      
    end
  end
end
